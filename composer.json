{
    "name": "maileryio/widget-form",
    "type": "library",
    "description": "Form Widget for Mailery Platform",
    "keywords": [
        "yii",
        "widget",
        "form"
    ],
    "homepage": "https://github.com/maileryio/widget-form",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/maileryio/widget-form",
        "issues": "https://github.com/maileryio/widget-form/issues",
        "wiki": "https://github.com/maileryio/widget-form/wiki"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "foxy/foxy": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.4",
        "form-manager/form-manager": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/validator": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Mailery\\Widget\\Form\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailery\\Widget\\Form\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
